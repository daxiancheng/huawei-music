  background-color:$bgcolor;\r\n    display: flow-root;\r\n    .header{\r\n        height: 90px;\r\n        text-align: center;\r\n        margin-top: 20px;\r\n        h1{\r\n            color:$color1;\r\n            font-size: 20px;\r\n            margin-bottom: 6px;\r\n        }\r\n        p{\r\n            color: $color2;\r\n            font-size: 12px;\r\n        }\r\n        .cricle{\r\n            display: flex;\r\n            justify-content: center;\r\n            align-items: center;\r\n            div{\r\n                width: 8px;\r\n                height: 8px;\r\n                border-radius: 50%;\r\n                background-color: $color3-1;\r\n                margin: 16px 4px;\r\n                &.smallCricle{\r\n                    width: 6px;\r\n                    height: 6px;\r\n                    background-color: $color3-2;\r\n                }\r\n            }\r\n        }\r\n    }\r\n    .mainShow{\r\n        height: calc(100vh - 270px);\r\n        display: flex;\r\n        flex-direction: column;\r\n        justify-content: center;\r\n        .ringBox{\r\n            display: flex;\r\n            justify-content: center;\r\n            align-items: center;\r\n            position: relative;\r\n            height: 40vh;\r\n            .outerRing{\r\n                width: 60vw;\r\n                height: 60vw;\r\n                position: absolute;\r\n                background: url('../svg/effect-move1.svg') no-repeat;\r\n                background-size: contain;\r\n                animation: rotateAnimation 7s linear  infinite reverse;\r\n            }\r\n            .middleRing{\r\n                width: 24vw;\r\n                height: 24vw;\r\n                position: absolute;\r\n                background: url('../svg/effect-move2.svg') no-repeat;\r\n                background-size: contain;\r\n                animation: rotateAnimation 5s linear  infinite;\r\n            }\r\n            .insideRing{\r\n                width: 70vw;\r\n                height: 70vw;\r\n                position: absolute;\r\n                background: url('../svg/effect-no-move.svg') no-repeat;\r\n                background-size: contain;\r\n                animation: rotateAnimation 9s linear  infinite;\r\n            }\r\n            @keyframes rotateAnimation {\r\n                0%{\r\n                    transform: rotate(0)\r\n                }\r\n                100%{\r\n                    transform: rotate(360deg)\r\n                }\r\n            }\r\n        }\r\n        .lyricsBox{\r\n            font-size: 14px;\r\n            text-align: center;\r\n            margin-top: 10px;\r\n            line-height: 1.6em;\r\n            .lyricsActive{\r\n                color: $color1;\r\n            }\r\n            .lyrics{\r\n                color: $color2;\r\n            }\r\n        }\r\n        \r\n    }\r\n    .footer{\r\n        height: 160px;\r\n        .svgBox{\r\n            display: flex;\r\n            justify-content: center;\r\n            svg{\r\n                width: 24px;\r\n                height: 24px;\r\n                margin: 0 20px;\r\n            }\r\n        }\r\n        .barBox{\r\n            display: flex;\r\n            padding: 0 16px;\r\n            justify-content: space-between;\r\n            align-items: center;\r\n            margin-top: 20px;\r\n            span{\r\n                color: $color2;\r\n                font-size: 12px;\r\n            }\r\n            .bar{\r\n                flex: 1;\r\n                height: 4px;\r\n                border-radius: 2px;\r\n                background-color:$color4;\r\n                margin: 0 6px;\r\n                .progressBar{\r\n                    height: 4px;\r\n                    width: 50px;\r\n                    border-radius: 2px;\r\n                    background-color: $color3;\r\n                    position: relative;\r\n                    &::after{\r\n                        content: '';\r\n                        width: 16px;\r\n                        height: 16px;\r\n                        background: url('../svg/progress.svg') no-repeat;\r\n                        background-size: contain;\r\n                        position: absolute;\r\n                        right: 0px;\r\n                        top: -6px;\r\n                    }\r\n                }\r\n            }\r\n        }\r\n        .controlButton{\r\n            display: flex;\r\n            justify-content: space-between;\r\n            padding: 0 16px;\r\n            margin-top: 20px;\r\n            align-items: center;\r\n            svg{\r\n                width: 28px;\r\n                height: 28px;\r\n                &.iconplay{\r\n                    width: 50px;\r\n                    height: 50px;\r\n                }\r\n            }\r\n        }\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"818350b501b763c750850897db977c9a","cacheData":{"env":{}}}